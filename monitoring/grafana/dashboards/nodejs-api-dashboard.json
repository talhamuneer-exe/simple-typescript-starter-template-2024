{
  "dashboard": {
    "title": "Node.js API Dashboard",
    "tags": ["nodejs", "api", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Request Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{route}} ({{status_code}})",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "reqps", "label": "Requests/sec" },
          { "format": "short" }
        ]
      },
      {
        "id": 2,
        "title": "HTTP Request Duration (95th percentile)",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{method}} {{route}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "s", "label": "Duration" },
          { "format": "short" }
        ]
      },
      {
        "id": 3,
        "title": "Active Requests",
        "type": "graph",
        "gridPos": { "h": 8, "w": 8, "x": 0, "y": 8 },
        "targets": [
          {
            "expr": "http_active_requests",
            "legendFormat": "Active Requests",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short" },
          { "format": "short" }
        ]
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 8, "x": 8, "y": 8 },
        "targets": [
          {
            "expr": "rate(http_errors_total[5m])",
            "legendFormat": "{{method}} {{route}} ({{error_code}})",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short" },
          { "format": "short" }
        ]
      },
      {
        "id": 5,
        "title": "Security Events",
        "type": "graph",
        "gridPos": { "h": 8, "w": 8, "x": 16, "y": 8 },
        "targets": [
          {
            "expr": "rate(security_events_total[5m])",
            "legendFormat": "{{event_type}} - {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short" },
          { "format": "short" }
        ]
      },
      {
        "id": 6,
        "title": "HTTP Status Codes",
        "type": "piechart",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
        "targets": [
          {
            "expr": "sum by (status_code) (rate(http_requests_total[5m]))",
            "legendFormat": "{{status_code}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 7,
        "title": "Node.js Memory Usage",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
        "targets": [
          {
            "expr": "nodejs_heap_size_total_bytes",
            "legendFormat": "Heap Total",
            "refId": "A"
          },
          {
            "expr": "nodejs_heap_size_used_bytes",
            "legendFormat": "Heap Used",
            "refId": "B"
          }
        ],
        "yaxes": [
          { "format": "bytes", "label": "Memory" },
          { "format": "short" }
        ]
      },
      {
        "id": 8,
        "title": "Rate Limit Hits",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
        "targets": [
          {
            "expr": "rate(rate_limit_hits_total[5m])",
            "legendFormat": "{{endpoint}} - {{ip}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short" },
          { "format": "short" }
        ]
      },
      {
        "id": 9,
        "title": "Response Time by Endpoint",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(response_time_seconds_bucket[5m]))",
            "legendFormat": "p50 - {{endpoint}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(response_time_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{endpoint}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(response_time_seconds_bucket[5m]))",
            "legendFormat": "p99 - {{endpoint}}",
            "refId": "C"
          }
        ],
        "yaxes": [
          { "format": "s", "label": "Response Time" },
          { "format": "short" }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}
